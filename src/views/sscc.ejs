    </div>
    <button onclick="gensscc()">submit</button>
    <div class="row">
      <div class="col-lg-12">
      <textarea rows="2" id="output" readonly style="border: none"></textarea>
      </div>
    </div>

  <script src="webapp-process.js"></script>
  <script src="checkdigit.js"></script>

